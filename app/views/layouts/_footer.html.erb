                        $(window).on('scroll', function() {
                            mobileNav.toggleClass('is-fixed', $(window).scrollTop() > mobileNavTopPosition);
                            $('.header-bottom').toggleClass('is-fixed', $(window).scrollTop() > mobileNavTopPosition);
                        });

                        if ($('#mobile-navigation .logo-mobile .logo').children().length == 0) {
                            $('#mobile-navigation .logo-mobile .logo').prepend($('.header-bottom .logo').html());
                        }
                        

                    } else {
                        $('body').removeClass('st-off-canvas');
                        $('#st-container').removeClass('st-effect-1 st-menu-open');

                        // main categories navigation fixed
                        var mainNav = $('#top-categories-menu'),
                            mainNavTopPosition = mainNav.offset().top;

